type Project {
  id: ID!
  userID: ID!
  name: String!
  category: String!
}

type Query {
  projects: [Project!]!
  project(id: ID!): Project
}

input NewProject {
  name: String!
  category: String!
}

type Mutation {
  createProject(input: NewProject!): Project!
  deleteProject(id: ID!, userID: ID!): ID!
  updateProject(id: ID!, input: NewProject!): Project!
}
